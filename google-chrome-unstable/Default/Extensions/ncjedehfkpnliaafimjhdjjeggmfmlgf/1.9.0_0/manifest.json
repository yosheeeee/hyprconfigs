{
   "action": {
      "default_title": "Click to open sidebar"
   },
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "_execute_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+K",
            "mac": "Command+Shift+K"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "shell/inject.js" ],
      "matches": [ "https://*.bing.com/edgesvc/shell*" ],
      "run_at": "document_start",
      "world": "MAIN"
   }, {
      "js": [ "libs/pdf.min.js", "content-script.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": true,
         "id": "ruleset_bing",
         "path": "rules/bing.json"
      } ]
   },
   "description": "Microsoft Copilot sidebar ported from Edge to Chrome",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Copilot sidebar for Chrome",
   "permissions": [ "sidePanel", "declarativeNetRequest", "tabs", "notifications" ],
   "side_panel": {
      "default_path": "sidepanel/index.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.9.0"
}
