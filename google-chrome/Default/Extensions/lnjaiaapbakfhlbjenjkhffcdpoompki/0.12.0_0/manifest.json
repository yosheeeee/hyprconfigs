{
   "action": {
      "default_popup": "popup.html",
      "default_title": "Catppuccin for GitHub File Explorer Icons"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content-scripts/content.js" ],
      "matches": [ "*://codeberg.org/*", "*://github.com/*", "*://gitlab.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Soothing pastel icons for GitHub File Explorer",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Catppuccin for GitHub File Explorer Icons",
   "optional_host_permissions": [ "*://*/*" ],
   "permissions": [ "storage", "contextMenus", "activeTab", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.12.0"
}
